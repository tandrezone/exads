{
    "repositories": [
        {
            "type": "path",
            "url": "packages/asciiArray"
        },
        {
            "type": "path",
            "url": "packages/tvSeries"
        },
        {
            "type": "path",
            "url": "packages/primeNumbers"
        },
        {
            "type": "path",
            "url": "packages/simplexdb"
        }
    ],
    "require": {
        "exads/ascii-array": "*@dev",
        "exads/tv-series": "*@dev",
        "exads/prime-numbers": "*@dev",
        "xbugs/simplexdb": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "exads\\primeNumbers\\": "packages/primeNumbers/src",
            "exads\\asciiArray\\": "packages/asciiArray/src",
            "exads\\tvSeries\\": "packages/tvSeries/src",
            "xbugs\\simplexdb\\": "packages/simplexdb/src"
        }
    },
    "autoload_dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}